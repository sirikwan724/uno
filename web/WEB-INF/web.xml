<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>addnewclothesservlet2</servlet-name>
        <servlet-class>addnewclothesservlet2</servlet-class>
    </servlet> 
    <servlet>
        <servlet-name>getclothesDataService</servlet-name>
        <servlet-class>services.getclothesDataService</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>addnewclothesservlet2</servlet-name>
        <url-pattern>/addnewclothesservlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getclothesDataService</servlet-name>
        <url-pattern>/getclothesDataService/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
